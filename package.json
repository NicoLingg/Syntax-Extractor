{
  "name": "syntax-extractor",
  "displayName": "Syntax Extractor",
  "description": "Hello there! ðŸ™Œ Meet the Syntax Extractor, my pet project. Crafted from navigating messy code and a desire for better AI communication. It's the breather for code enthusiasts like you.",
  "version": "0.1.9",
  "publisher": "JosefNobach",
  "icon": "dist/resources/SyntaxExtractor256x256.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/jojomondag/syntaxExtractor.git"
  },
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "configuration": {
      "title": "Syntax Extractor",
        "properties": {
            "syntax-extractor.compressionLevel": {
                "type": "string",
                "default": "medium",
                "description": "The compression level."
            },
            "syntax-extractor.inputTextBoxHeight": {
                "type": "string",
                "default": "100px",
                "description": "The height of the input text box."
            },
            "syntax-extractor.fileTypes": {
                "type": "array",
                "default": [".txt", ".js", ".ts", ".html"],
                "description": "The file types to read."
            },
            "syntax-extractor.excludedPaths": {
                "type": "array",
                "default": ["\\.git", "\\node_modules", "\\.eslintrc.json", "\\.gitignore", "\\.vscodeignore", "\\CHANGELOG.md", "\\package-lock.json", "\\README.md"],
                "description": "The paths to exclude."
            }
        }
    },
    "commands": [
      {
        "command": "syntaxExtractor.openGui",
        "title": "Syntax Extractor Settings"
      },
      {
        "command": "syntaxExtractor.extractAndCopyText",
        "title": "Extract and Copy Text",
        "category": "Syntax Extractor"
      },
      {
        "command": "syntaxExtractor.extractFileFolderTree",
        "title": "Extract File/Folder Tree",
        "category": "Syntax Extractor"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "syntaxExtractor.extractAndCopyText",
          "group": "0_navigation@0"
        },
        {
          "command": "syntaxExtractor.extractFileFolderTree",
          "group": "0_navigation@1"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "syntaxExtractorButton",
          "title": "SE",
          "icon": "dist/resources/SyntaxExtractor256x256.png"
        }
      ]
    },
    "views": {
      "syntaxExtractorButton": [
        {
          "id": "syntaxExtractorView",
          "name": "Syntax Extractor"
        }
      ]
    }
  },
  "scripts": {
    "webpack": "webpack --config webpack.config.js",
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.2",
    "@types/node": "^20.11.25",
    "@types/react": "^18.2.36",
    "@types/react-dom": "^18.2.14",
    "@types/vscode": "^1.83.0",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "@vscode/test-electron": "^2.3.4",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "eslint": "^8.50.0",
    "glob": "^10.3.3",
    "html-webpack-plugin": "^5.5.3",
    "mocha": "^10.2.0",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.4.4",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "tiktoken": "^1.0.10"
  }
}
