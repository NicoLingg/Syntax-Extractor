{
    "name": "syntax-extractor",
    "displayName": "Syntax Extractor",
    "description": "Syntax Extractor, helps you Gather your Code",
    "version": "0.3.2",
    "publisher": "JosefNobach",
    "icon": "out/resources/SyntaxExtractor256x256.png",
    "repository": {
      "type": "git",
      "url": "https://github.com/jojomondag/syntaxExtractor.git"
    },
    "engines": {
      "vscode": "^1.87.0"
    },
    "categories": [
      "Other"
    ],
    "main": "./extension.js",
    "contributes": {
        "commands": [
            {
                "command": "codeExtractor.extractCode",
                "title": "Extract Code Structure"
            }
        ],
        "menus": {
            "explorer/context": [
                {
                    "command": "codeExtractor.extractCode",
                    "when": "explorerResourceIsFolder",
                    "group": "navigation"
                }
            ]
        }
    },
    "scripts": {
        "lint": "eslint .",
        "pretest": "npm run lint",
        "test": "node ./test/runTest.js"
    },
    "devDependencies": {
        "@types/vscode": "^1.60.0",
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.2.2",
        "@types/node": "14.x",
        "eslint": "^7.32.0",
        "glob": "^7.1.7",
        "mocha": "^9.1.1",
        "typescript": "^4.4.3",
        "@vscode/test-electron": "^1.6.2"
    }
}