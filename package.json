{
  "name": "syntax-extractor",
  "displayName": "Syntax Extractor",
  "publisher": "JosefNobach",
  "repository": {
    "type": "git",
    "url": "https://github.com/jojomondag/syntaxExtractor.git"
  },
  "icon": "dist/resources/SyntaxExtractor256x256.png",
  "description": "Hello there! ðŸ™Œ Meet the Syntax Extractor, my pet project. Crafted from navigating messy code and a desire for better AI communication. It's the breather for code enthusiasts like you.",
  "version": "0.0.5",
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "syntaxExtractor.openGui",
        "title": "Open GUI"
      },
      {
        "command": "syntaxExtractor.extractAndCopyText",
        "title": "extractAndCopyText",
        "category": "Syntax Extractor"
      },
      {
        "command": "syntaxExtractor.extractFileFolderTree",
        "title": "extractFileFolderTree",
        "category": "Syntax Extractor"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "syntaxExtractor.extractAndCopyText",
          "group": "0_navigation@0"
        },
        {
          "command": "syntaxExtractor.extractFileFolderTree",
          "group": "0_navigation@0"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "syntaxExtractorButton",
          "title": "SE",
          "icon": "dist/resources/SyntaxExtractor256x256.png"
        }
      ]
    },
    "views": {
      "syntaxExtractorButton": [
        {
          "id": "syntaxExtractorView",
          "name": "Syntax Extractor"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.2",
    "@types/node": "^20.8.7",
    "@types/vscode": "^1.83.0",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "@vscode/test-electron": "^2.3.4",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.50.0",
    "glob": "^10.3.3",
    "html-webpack-plugin": "^5.5.3",
    "mocha": "^10.2.0",
    "ts-loader": "^9.4.4",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  }
}